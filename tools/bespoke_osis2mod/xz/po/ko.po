#~ msgid "  Streams:            %s\n"
#~ msgstr "  스트림:            %s\n"

#~ msgid "  Blocks:             %s\n"
#~ msgstr "  블록:             %s\n"

#~ msgid "  Ratio:              %s\n"
#~ msgstr "  압축률:              %s\n"

#~ msgid "  Check:              %s\n"
#~ msgstr "  검사:              %s\n"

#~ msgid ""
#~ "  Streams:\n"
#~ "    Stream    Blocks      CompOffset    UncompOffset        CompSize      UncompSize  Ratio  Check      Padding"
#~ msgstr ""
#~ "  스트림:\n"
